\newtheoremstyle{note} 
{16pt}  % Platz zwischen Kopf und voherigem Text 
{16pt}  % und nachfolgendem Text 
{}     % Schriftart des Koerpers 
{}     % mit \parindent Einzug 
{\bf}  % Schriftart des Kopfes 
{:}     % Nach Bedarf z.B. Doppelpunkt nach dem Kopf 
{0.5em} % Platz zwischen Kopf und Koerper 
{}     % Kopfname 

\theoremstyle{note} 
\newtheorem{aufgabe}{Aufgabe} 



\begin{document}
\begin{flushleft}
\begin{aufgabe} ~  
$$f(x)=x^{3}$$ 
{\bf Funktion und Ableitung:} 
\begin{align*} 
f(x)&=x^{3}\\ 
f'(x)&=3 x^{2}\\ 
f''(x)&=6 x\\ 
f'''(x)&=6\\ 
\end{align*} 
{\bf Verhalten im Unendlichen:} 
\[ \lim_{x\rightarrow -\infty} f(x) =-\infty\]\[ \lim_{x\rightarrow\infty} f(x) =\infty\]{\bf Nullstellen:} 
\begin{align*} 
f(x)&=0 \\ 
\text{mit CAS:\qquad}x&=0\end{align*} 
$\Rightarrow$ Nullstelle bei $N(0|0)$ \\ 
{\bf Symmetrie:} 
\[ f(-x)=(-x)^{3}=- x^{3}=-(x^{3})=-f(x) \]\[ \Rightarrow \text{punktsymmetrisch zum Ursprung} \]{\bf Extrempunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f'(x)&=& &0& \\ 
\text{mit CAS: \qquad}x&=& &0& \quad &:& \quad f(0)&=& \, &0\\ 
\end{alignat*} 
{\bf Art der Extrempunkte ermitteln:} \\[1em] 
{\em 1. M\"oglichkeit:} Funktionswerte in einer gen\"ugend kleinen Umgebung vergleichen \\ 
$$f(-0.1)=-0.001  <  f(0)=0  <   f(0.1)=0.001$$\\ 
$\Rightarrow$ Sattelpunkt bei $(0|0)$ \\ 
\vspace{1em}{\em 2. M\"oglichkeit:} Monotonieverhalten untersuchen \\ 
$$f'(-0.1)=0.03\quad \Rightarrow \text{ monoton steigend} $$$$f'(0)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(0.1)=0.03\quad \Rightarrow \text{ monoton steigend} $$$\Rightarrow$ Sattelpunkt bei $(0|0)$ \\ 
\vspace{1em}{\em 3. M\"oglichkeit:} Kr\"ummungsverhalten untersuchen \\ 
$$f''(0)=0$$ 
$\Rightarrow$ Keine Entscheidung m\"oglich. \\ 
{\bf Wendepunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f''(x)&=& &0& \\ 
\text{mit CAS:\qquad}x&=& &0& \quad &:& \quad f(0)&=& \, &0\\ 
\end{alignat*} 
{\em 1. M\"oglichkeit:} 3. Ableitung untersuchen \\ 
$$f'''(0)=6.0\neq 0 $$ 
$\Rightarrow$ Wendepunkt bei $W(0|0)$ \\ 
{\em 2. M\"oglichkeit:} 2. Ableitung auf Vorzeichenwechsel untersuchen. \\ 
$$f''(-0.1)=-0.6\quad \Rightarrow \text{ Rechtskr\"ummung} $$$$f''(0.1)=0.6\quad \Rightarrow \text{ Linkskr\"ummung} $$$\Rightarrow$ Wendepunkt bei $W(0|0)$ \\ 
\vspace{1em}{\bf Graph:} 
\definecolor{cqcqcq}{rgb}{0.75,0.75,0.75} 
\begin{tikzpicture}[domain=-5:5],scale=1][line cap=round,line join=round,>=triangle 45,x=1.0cm,y=1.0cm]\draw[color=black] plot[smooth] function{x**3}; 
\end{tikzpicture}
\end{aufgabe}\clearpage\begin{aufgabe} ~  
$$f(x)=- 4 x^{3} + 3 x^{2}$$ 
{\bf Funktion und Ableitung:} 
\begin{align*} 
f(x)&=- 4 x^{3} + 3 x^{2}\\ 
f'(x)&=- 12 x^{2} + 6 x\\ 
f''(x)&=6 \left(- 4 x + 1\right)\\ 
f'''(x)&=-24\\ 
\end{align*} 
{\bf Verhalten im Unendlichen:} 
\[ \lim_{x\rightarrow -\infty} f(x) =\infty\]\[ \lim_{x\rightarrow\infty} f(x) =-\infty\]{\bf Nullstellen:} 
\begin{align*} 
f(x)&=0 \\ 
\text{mit CAS:\qquad}x_1&=0\\ 
x_2&=0.75\\ 
\end{align*} 
$\Rightarrow$ Nullstelle bei $N_1(0|0)$ \\ 
$\Rightarrow$ Nullstelle bei $N_2(0.75|0)$ \\ 
{\bf Symmetrie:} 
keine Symmetrie \\ 
{\bf Extrempunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f'(x)&=& &0& \\ 
\text{mit CAS: \qquad}x_1&=& &0& \quad &:& \quad &f(0)&=& \,0\\ 
x_2&=& &0.5& \quad &:& \quad &f(0.5)&=& \,0.25\\ 
\end{alignat*} 
{\bf Art der Extrempunkte ermitteln:} \\[1em] 
{\em 1. M\"oglichkeit:} Funktionswerte in einer gen\"ugend kleinen Umgebung vergleichen \\ 
$$f(-0.1)=0.034  >  f(0)=0  <   f(0.1)=0.026$$\\ 
$\Rightarrow$ Tiefpunkt bei $(0|0)$ \\ 
\vspace{1em}$$f(0.4)=0.224  <  f(0.5)=0.25  >  f(0.6)=0.216$$\\ 
$\Rightarrow$ Hochpunkt bei $(0.5|0.25)$ \\ 
\vspace{1em}{\em 2. M\"oglichkeit:} Monotonieverhalten untersuchen \\ 
$$f'(-0.1)=-0.72\quad \Rightarrow \text{ monoton fallend} $$$$f'(0)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(0.1)=0.48\quad \Rightarrow \text{ monoton steigend} $$$\Rightarrow$ Tiefpunkt bei $(0|0)$ \\ 
\vspace{1em}$$f'(0.4)=0.48\quad \Rightarrow \text{ monoton steigend} $$$$f'(0.5)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(0.6)=-0.72\quad \Rightarrow \text{ monoton fallend} $$$\Rightarrow$ Hochpunkt bei $(0.5|0.25)$ \\ 
\vspace{1em}{\em 3. M\"oglichkeit:} Kr\"ummungsverhalten untersuchen \\ 
$$f''(0)=6.0> 0 $$ 
$\Rightarrow$ Tiefpunkt bei $T(0|0)$ \\ 
$$f''(0.5)=-6.0< 0 $$ 
$\Rightarrow$ Hochpunkt bei $H(0.5|0.25)$ \\ 
{\bf Wendepunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f''(x)&=& &0& \\ 
\text{mit CAS:\qquad}x&=& &0.25& \quad &:& \quad f(0.25)&=& \, &0.13\\ 
\end{alignat*} 
{\em 1. M\"oglichkeit:} 3. Ableitung untersuchen \\ 
$$f'''(0.25)=-24.0\neq 0 $$ 
$\Rightarrow$ Wendepunkt bei $W(0.25|0.13)$ \\ 
{\em 2. M\"oglichkeit:} 2. Ableitung auf Vorzeichenwechsel untersuchen. \\ 
$$f''(0.15)=2.4\quad \Rightarrow \text{ Linkskr\"ummung} $$$$f''(0.35)=-2.4\quad \Rightarrow \text{ Rechtskr\"ummung} $$$\Rightarrow$ Wendepunkt bei $W(0.25|0.13)$ \\ 
\vspace{1em}{\bf Graph:} 
\definecolor{cqcqcq}{rgb}{0.75,0.75,0.75} 
\begin{tikzpicture}[domain=-5:5],scale=1][line cap=round,line join=round,>=triangle 45,x=1.0cm,y=1.0cm]\draw[color=black] plot[smooth] function{-4*x**3 + 3*x**2}; 
\end{tikzpicture}
\end{aufgabe}\clearpage\begin{aufgabe} ~  
$$f(x)=- 5 x^{3} + 5 x$$ 
{\bf Funktion und Ableitung:} 
\begin{align*} 
f(x)&=- 5 x^{3} + 5 x\\ 
f'(x)&=- 15 x^{2} + 5\\ 
f''(x)&=- 30 x\\ 
f'''(x)&=-30\\ 
\end{align*} 
{\bf Verhalten im Unendlichen:} 
\[ \lim_{x\rightarrow -\infty} f(x) =\infty\]\[ \lim_{x\rightarrow\infty} f(x) =-\infty\]{\bf Nullstellen:} 
\begin{align*} 
f(x)&=0 \\ 
\text{mit CAS:\qquad}x_1&=-1.0\\ 
x_2&=0\\ 
x_3&=1.0\\ 
\end{align*} 
$\Rightarrow$ Nullstelle bei $N_1(-1.0|0)$ \\ 
$\Rightarrow$ Nullstelle bei $N_2(0|0)$ \\ 
$\Rightarrow$ Nullstelle bei $N_3(1.0|0)$ \\ 
{\bf Symmetrie:} 
\[ f(-x)=- 5 (-x)^{3} + 5 (-x)=5 x^{3} - 5 x=-(- 5 x^{3} + 5 x)=-f(x) \]\[ \Rightarrow \text{punktsymmetrisch zum Ursprung} \]{\bf Extrempunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f'(x)&=& &0& \\ 
\text{mit CAS: \qquad}x_1&=& &-0.58& \quad &:& \quad &f(-0.58)&=& \,-1.9\\ 
x_2&=& &0.58& \quad &:& \quad &f(0.58)&=& \,1.9\\ 
\end{alignat*} 
{\bf Art der Extrempunkte ermitteln:} \\[1em] 
{\em 1. M\"oglichkeit:} Funktionswerte in einer gen\"ugend kleinen Umgebung vergleichen \\ 
$$f(-0.68)=-1.83  >  f(-0.58)=-1.92  <   f(-0.48)=-1.84$$\\ 
$\Rightarrow$ Tiefpunkt bei $(-0.58|-1.9)$ \\ 
\vspace{1em}$$f(0.48)=1.84  <  f(0.58)=1.92  >  f(0.68)=1.83$$\\ 
$\Rightarrow$ Hochpunkt bei $(0.58|1.9)$ \\ 
\vspace{1em}{\em 2. M\"oglichkeit:} Monotonieverhalten untersuchen \\ 
$$f'(-0.68)=-1.88\quad \Rightarrow \text{ monoton fallend} $$$$f'(-0.58)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(-0.48)=1.58\quad \Rightarrow \text{ monoton steigend} $$$\Rightarrow$ Tiefpunkt bei $(-0.58|-1.9)$ \\ 
\vspace{1em}$$f'(0.48)=1.58\quad \Rightarrow \text{ monoton steigend} $$$$f'(0.58)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(0.68)=-1.88\quad \Rightarrow \text{ monoton fallend} $$$\Rightarrow$ Hochpunkt bei $(0.58|1.9)$ \\ 
\vspace{1em}{\em 3. M\"oglichkeit:} Kr\"ummungsverhalten untersuchen \\ 
$$f''(-0.58)=17.0> 0 $$ 
$\Rightarrow$ Tiefpunkt bei $T(-0.58|-1.9)$ \\ 
$$f''(0.58)=-17.0< 0 $$ 
$\Rightarrow$ Hochpunkt bei $H(0.58|1.9)$ \\ 
{\bf Wendepunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f''(x)&=& &0& \\ 
\text{mit CAS:\qquad}x&=& &0& \quad &:& \quad f(0)&=& \, &0\\ 
\end{alignat*} 
{\em 1. M\"oglichkeit:} 3. Ableitung untersuchen \\ 
$$f'''(0)=-30.0\neq 0 $$ 
$\Rightarrow$ Wendepunkt bei $W(0|0)$ \\ 
{\em 2. M\"oglichkeit:} 2. Ableitung auf Vorzeichenwechsel untersuchen. \\ 
$$f''(-0.1)=3.0\quad \Rightarrow \text{ Linkskr\"ummung} $$$$f''(0.1)=-3.0\quad \Rightarrow \text{ Rechtskr\"ummung} $$$\Rightarrow$ Wendepunkt bei $W(0|0)$ \\ 
\vspace{1em}{\bf Graph:} 
\definecolor{cqcqcq}{rgb}{0.75,0.75,0.75} 
\begin{tikzpicture}[domain=-5:5],scale=1][line cap=round,line join=round,>=triangle 45,x=1.0cm,y=1.0cm]\draw[color=black] plot[smooth] function{-5*x**3 + 5*x}; 
\end{tikzpicture}
\end{aufgabe}\clearpage\begin{aufgabe} ~  
$$f(x)=- 3 x^{3} - x^{2} - 2 x$$ 
{\bf Funktion und Ableitung:} 
\begin{align*} 
f(x)&=- 3 x^{3} - x^{2} - 2 x\\ 
f'(x)&=- 9 x^{2} - 2 x - 2\\ 
f''(x)&=- 2 \left(9 x + 1\right)\\ 
f'''(x)&=-18\\ 
\end{align*} 
{\bf Verhalten im Unendlichen:} 
\[ \lim_{x\rightarrow -\infty} f(x) =\infty\]\[ \lim_{x\rightarrow\infty} f(x) =-\infty\]{\bf Nullstellen:} 
\begin{align*} 
f(x)&=0 \\ 
\text{mit CAS:\qquad}x&=0\end{align*} 
$\Rightarrow$ Nullstelle bei $N(0|0)$ \\ 
{\bf Symmetrie:} 
keine Symmetrie \\ 
{\bf Extrempunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f'(x)&=& &0& \\ 
\text{mit CAS: \qquad}\end{alignat*} 
Keine L\"osung $\Rightarrow$ Keine Extrempunkte \\ 
{\bf Art der Extrempunkte ermitteln:} \\[1em] 
{\em 1. M\"oglichkeit:} Funktionswerte in einer gen\"ugend kleinen Umgebung vergleichen \\ 
{\em 2. M\"oglichkeit:} Monotonieverhalten untersuchen \\ 
{\em 3. M\"oglichkeit:} Kr\"ummungsverhalten untersuchen \\ 
{\bf Wendepunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f''(x)&=& &0& \\ 
\text{mit CAS:\qquad}x&=& &-0.11& \quad &:& \quad f(-0.11)&=& \, &0.21\\ 
\end{alignat*} 
{\em 1. M\"oglichkeit:} 3. Ableitung untersuchen \\ 
$$f'''(-0.11)=-18.0\neq 0 $$ 
$\Rightarrow$ Wendepunkt bei $W(-0.11|0.21)$ \\ 
{\em 2. M\"oglichkeit:} 2. Ableitung auf Vorzeichenwechsel untersuchen. \\ 
$$f''(-0.21)=1.8\quad \Rightarrow \text{ Linkskr\"ummung} $$$$f''(-0.011)=-1.8\quad \Rightarrow \text{ Rechtskr\"ummung} $$$\Rightarrow$ Wendepunkt bei $W(-0.11|0.21)$ \\ 
\vspace{1em}{\bf Graph:} 
\definecolor{cqcqcq}{rgb}{0.75,0.75,0.75} 
\begin{tikzpicture}[domain=-5:5],scale=1][line cap=round,line join=round,>=triangle 45,x=1.0cm,y=1.0cm]\draw[color=black] plot[smooth] function{-3*x**3 - x**2 - 2*x}; 
\end{tikzpicture}
\end{aufgabe}\clearpage\begin{aufgabe} ~  
$$f(x)=2 x^{4}$$ 
{\bf Funktion und Ableitung:} 
\begin{align*} 
f(x)&=2 x^{4}\\ 
f'(x)&=8 x^{3}\\ 
f''(x)&=24 x^{2}\\ 
f'''(x)&=48 x\\ 
\end{align*} 
{\bf Verhalten im Unendlichen:} 
\[ \lim_{x\rightarrow -\infty} f(x) =\infty\]\[ \lim_{x\rightarrow\infty} f(x) =\infty\]{\bf Nullstellen:} 
\begin{align*} 
f(x)&=0 \\ 
\text{mit CAS:\qquad}x&=0\end{align*} 
$\Rightarrow$ Nullstelle bei $N(0|0)$ \\ 
{\bf Symmetrie:} 
\[ f(-x)=2 (-x)^{4}=2 x^{4}=f(x) \]\[ \Rightarrow \text{achsensymmetrich zur $y$-Achse} \]{\bf Extrempunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f'(x)&=& &0& \\ 
\text{mit CAS: \qquad}x&=& &0& \quad &:& \quad f(0)&=& \, &0\\ 
\end{alignat*} 
{\bf Art der Extrempunkte ermitteln:} \\[1em] 
{\em 1. M\"oglichkeit:} Funktionswerte in einer gen\"ugend kleinen Umgebung vergleichen \\ 
$$f(-0.1)=0.0002  >  f(0)=0  <   f(0.1)=0.0002$$\\ 
$\Rightarrow$ Tiefpunkt bei $(0|0)$ \\ 
\vspace{1em}{\em 2. M\"oglichkeit:} Monotonieverhalten untersuchen \\ 
$$f'(-0.1)=-0.008\quad \Rightarrow \text{ monoton fallend} $$$$f'(0)=0 \quad \Rightarrow \text{ waagerechte Tangente} $$$$f'(0.1)=0.008\quad \Rightarrow \text{ monoton steigend} $$$\Rightarrow$ Tiefpunkt bei $(0|0)$ \\ 
\vspace{1em}{\em 3. M\"oglichkeit:} Kr\"ummungsverhalten untersuchen \\ 
$$f''(0)=0$$ 
$\Rightarrow$ Keine Entscheidung m\"oglich. \\ 
{\bf Wendepunkte (Notwendige Bedingung):} 
\begin{alignat*}{7} 
f''(x)&=& &0& \\ 
\text{mit CAS:\qquad}x&=& &0& \quad &:& \quad f(0)&=& \, &0\\ 
\end{alignat*} 
{\em 1. M\"oglichkeit:} 3. Ableitung untersuchen \\ 
$$f'''(0)=0$$ 
$\Rightarrow$ Keine Entscheidung m\"oglich. \\ 
{\em 2. M\"oglichkeit:} 2. Ableitung auf Vorzeichenwechsel untersuchen. \\ 
$$f''(-0.1)=0.24\quad \Rightarrow \text{ Linkskr\"ummung} $$$$f''(0.1)=0.24\quad \Rightarrow \text{ Linkskr\"ummung} $$Kein Wendepunkt. \\[1em] 
