## Generate a table in latex-format with given values

from sympy.abc import *
from skelet import Skelet

class Table:
    def centered(self):
        lf = open('datei.tex','a')
        lf.write('\
\\begin{tabular}{c|c|c|c} \n\
test & test & test & test \\\\ \\hline \n\
t2 & t2 & t2 & t2 \n\
\\end{tabular} \n')




