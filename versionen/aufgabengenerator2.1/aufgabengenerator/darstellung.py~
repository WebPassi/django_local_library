## Generate a table in latex-format with given values

from sympy.abc import *
from sympy import *
from elements import *
from functions import *
import os
import random

def zeigeFunktion(funktion):
    dars=funktion.name + "(" + funktion.variable + ")=" + latex(funktion.expr)
    return dars













